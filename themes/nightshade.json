{
  "name": "Nightshade",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#dcdcd0",
        "background": "#222222",
        "lineHighlight": "#3c3c3c80",
        "selection": "#3160a680",
        "activeLinkForeground": "#5fa7ef",
        "currentFindMatchHighlight": "#3160a680",
        "findMatchHighlight": "#5e314480",
        "findRangeHighlight": "#5e314480",
        "guide": "#3c3c3c80",
        "greensquiggly": "#e7ad58",
        "redsquiggly": "#dc7363",
        "hoverHighlight": "#1c8dff50",
        "inactiveSelection": "#3160a680",
        "rangeHighlight": "#3c3c3c80",
        "referenceHighlight": "#5e314480",
        "wordHighlight": "#56565680",
        "wordHighlightStrong": "#56565680"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7f9f7f"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#c5d47f"
      }
    },
    {
      "scope": "meta.tag.sgml.doctype.html",
      "settings": {
        "foreground": "#bf86a5"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.class.mixin",
        "entity.other.attribute-name.attribute",
