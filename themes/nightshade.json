      "settings": {
        "foreground": "#bf86a5"
      }
    },
    {
      "scope": [
        "keyword.other.important"
      ],
      "settings": {
        "foreground": "#bf86a5"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#af7171",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dfaf8f"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dfaf8f"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#5fa7ef"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc7363"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e7ad58"
      }
    },
    {
      "scope": [
        "beginning.punctuation.definition.quote.markdown",
        "beginning.punctuation.definition.list.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.constant.markdown",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.link.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.separator.key-value.markdown",
        "meta.link.reference.markdown"
      ],
      "settings": {
        "foreground": "#878780"
      }
    },
    {
      "scope": [
        "markup.quote.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#7f9f7f"
      }
    },
    {
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#cc9393",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#c5d47f"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#878780"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": [
        "meta.preprocessor.macro",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#dfaf8f"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#cc9393"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#c5d47f"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#878780"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#cc9393"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less"
      ],
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.metatag.php",
        "punctuation.section.embedded.end.metatag.php"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#bf86a5"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.class",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.return.type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#8cd0d3"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "foreground": "#dfaf8f"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "foreground": "#dfaf8f"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#dfaf8f"
      }
    },
    {
      "scope": "punctuation.definition.comment.line.shebang.shell",
      "settings": {
        "foreground": "#7f9f7f"
      }
    },
    {
      "scope": [
        "string.interpolated.backtick.shell",
        "comment.line.shebang.shell"
      ],
      "settings": {
        "foreground": "#c5d47f"
      }
    },
    {
      "scope": "support.function.builtin.shell",
      "settings": {
        "foreground": "#bf86a5"
      }
    },
    {
      "scope": "punctuation.definition.variable.shell",
      "settings": {
        "foreground": "#f0dfaf"
      }
    },
    {
      "scope": [
        "storage.type.class.jsdoc",
        "entity.name.type.instance.jsdoc"
      ],
      "settings": {
        "foreground": "#afd8af"
      }
    },
    {
      "scope": [
        "variable.other.jsdoc"
      ],
      "settings": {
        "foreground": "#c5d47f"
      }
    }
  ],
  "colors": {
    "terminal.ansiBlack": "#222222",
    "terminal.ansiBrightBlack": "#6f6f6b",
    "terminal.ansiRed": "#9c574f",
    "terminal.ansiBrightRed": "#dc7363",
    "terminal.ansiGreen": "#368f57",
    "terminal.ansiBrightGreen": "#a7eb5f",
    "terminal.ansiYellow": "#e7ad58",
    "terminal.ansiBrightYellow": "#e0d35f",
    "terminal.ansiBlue": "#327cb5",
    "terminal.ansiBrightBlue": "#5fa7ef",
    "terminal.ansiMagenta": "#8f5f8f",
    "terminal.ansiBrightMagenta": "#bc7cac",
    "terminal.ansiCyan": "#4faba3",
    "terminal.ansiBrightCyan": "#6cd0d3",
    "terminal.ansiWhite": "#babab0",
    "terminal.ansiBrightWhite": "#dcdcd0"
  }
}
